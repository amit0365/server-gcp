{
    "compilerOptions": {
      "target": "es2020",
      "module": "ESNext",
      "esModuleInterop": true,
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react-jsx",
      "types": [
        "node"
      ],
      "incremental": true
    },
    "include": [
      "src",
      "src/input_gen/"
    ],
    "exclude": [
      "node_modules"
    ]
}
  